{
    "name": "Extension-101-2",
    "description": "Open any bookmarked tab",
    "version": "1.0.0",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html"
    },
    "permissions": [
        "bookmarks"
    ],
    "icons": {
        "16": "ic_launcher/16.png",
        "32": "ic_launcher/32.png",
        "48": "ic_launcher/48.png",
        "128": "ic_launcher/128.png"
    }
}
